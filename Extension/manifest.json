{
  "manifest_version": 3,
  "name": "Fun Counter with",
  "version": "0.001",
  "content_scripts": [
    { "matches": ["<all_urls>"], "css": ["./content/index.css"], "js": ["./content/content.js", "./content/main.js"] }
  ],
  "background": {
    "service_worker": "./background/background.js"
  },
  "action": {
   
    "default_title": "Fun Counter"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
